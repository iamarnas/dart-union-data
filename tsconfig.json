{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": [
			"es6",
			"ES2022"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"moduleResolution": "node",
		"noImplicitAny": true,
	},
	"exclude": [
		"./node_modules",
		".vscode-test"
	],
	"include": [
		"./src/**/*"
	]
}