{
	"name": "dart-union-data",
	"displayName": "dart union data",
	"description": "A simple way to create Dart programming language union data.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"activationEvents": [
		"onLanguage:dart"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": []
	},
	"scripts": {
		"start": "npm run build && node ./out/extension.js",
		"clean:nyc": "rm -rf .nyc_output coverage",
		"build": "rm -rf out && tsc",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "^17.0.35",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
		"@typescript-eslint/parser": "^5.26.0",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"nyc": "^15.1.0",
		"typescript": "^4.7.2",
		"vscode-test": "^1.4.1"
	}
}